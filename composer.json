{
    "name": "mmo/yii2-utils",
    "description": "A collection of extensions for Yii Framework 2.0",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Marcin Morawski",
            "email": "marcin@morawskim.pl"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.2",
        "yiisoft/yii2": "~2.0.14"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.3"
    },
    "autoload": {
        "psr-4": {
            "mmo\\yii2\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "mmo\\yii2\\tests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "composer",
            "url": "https://asset-packagist.org"
        }
    ]
}
